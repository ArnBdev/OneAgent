{
  "name": "oneagent-core",
  "version": "4.0.2",
  "description": "OneAgent CoreAgent - ModulÃ¦r AI-agentplattform with UI",
  "main": "dist/coreagent/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/coreagent/main.js",
    "server:unified": "npx tsx coreagent/server/unified-mcp-server.ts",
    "server:unified:dev": "npx nodemon --exec npx tsx coreagent/server/unified-mcp-server.ts",
    "memory:server": "python servers/oneagent_memory_server.py",
  "test": "node scripts/dev-utils.cjs test",
    "test:api": "ts-node tests/test-real-api.ts",
    "test:key": "ts-node tests/test-api-key.ts",
    "test:imports": "ts-node tests/test-import.ts",
    "test:embeddings": "ts-node tests/test-real-api.ts",
    "test:mcp": "ts-node tests/test-mcp-http.ts",
    "clean": "node scripts/dev-utils.js clean",
    "setup": "node scripts/dev-utils.js setup",
    "docs": "node scripts/dev-utils.js docs",
    "dev-mode": "node scripts/dev-utils.js dev",
    "create": "node scripts/dev-utils.js create",
    "new:test": "node scripts/dev-utils.js create test",
    "new:doc": "node scripts/dev-utils.js create doc",
    "new:script": "node scripts/dev-utils.js create script",
    "check-structure": "node scripts/ensure-structure.js scan",
    "structure-guide": "node scripts/ensure-structure.js guide",
    "create-file": "node scripts/ensure-structure.js create",
    "validate-structure": "node scripts/validate-project-structure.js",
    "fix-structure": "node scripts/fix-project-structure.js",
    "fix-structure:dry": "node scripts/fix-project-structure.js --dry-run",
    "fix-structure:interactive": "node scripts/fix-project-structure.js --interactive",
    "precommit": "npm run validate-structure",
    "lint": "eslint . --ext .ts --fix",
    "lint:check": "eslint . --ext .ts",
    "type-check": "tsc --noEmit",
  "verify": "npm run type-check && npm run lint:check",
  "verify:runtime": "npm run verify && npm run smoke:runtime",
  "verify:final": "ts-node scripts/final-verification.ts",
  "guardrails": "node scripts/guardrails-scan.cjs",
  "smoke": "ts-node tests/canonical/unified-systems.smoke.test.ts",
  "smoke:mcp": "ts-node tests/smoke/mcp-startup.smoke.ts",
  "smoke:runtime": "ts-node scripts/runtime-smoke.ts",
  "demo:hello": "ts-node scripts/hello-a2a.ts",
  "format": "npx prettier --write .",
    "ui:dev": "vite",
    "ui:build": "vite build",
  "ui:preview": "vite preview",
  "test:ops": "node ./tests/monitoring/operation-metrics-summary.test.ts"
  },
  "keywords": [
    "ai",
    "agent",
    "automation",
    "workflow",
    "mcp",
    "typescript",
    "react",
    "ui"
  ],
  "author": "Arne",
  "license": "MIT",
  "type": "commonjs",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@chroma-core/default-embed": "^0.1.8",
    "@google/generative-ai": "^0.24.1",
    "@types/passport": "^1.0.17",
    "@types/uuid": "^10.0.0",
    "chalk": "^5.4.1",
    "chromadb": "^3.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.263.1",
    "mem0ai": "^2.1.33",
    "node-fetch": "^2.7.0",
    "passport": "^0.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^11.1.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/axios": "^0.14.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.20",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "axios": "^1.9.0",
    "eslint": "^9.30.1",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.36.0",
  "vite": "^4.4.0",
  "tsx": "^4.20.4"
  }
}
