{
  "name": "oneagent-core",
  "version": "4.1.1",
  "description": "OneAgent CoreAgent - ModulÃ¦r AI-agentplattform with UI",
  "private": true,
  "main": "dist/coreagent/main.js",
  "packageManager": "npm@11.0.0",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/coreagent/main.js",
    "server:unified": "node -r ts-node/register coreagent/server/unified-mcp-server.ts",
    "server:stdio": "node -r ts-node/register coreagent/server/unified-mcp-stdio.ts",
    "server:unified:dev": "nodemon --exec node -r ts-node/register coreagent/server/unified-mcp-server.ts",
    "memory:server": "python servers/oneagent_memory_server.py",
    "servers:start": "npm-run-all -p memory:server server:unified",
    "servers:start:quiet": "cross-env ONEAGENT_MCP_QUIET=1 npm-run-all -p memory:server server:unified",
    "workspace:clean": "node scripts/cleanup-workspace.cjs",
    "python:venv": "pwsh -NoProfile -Command \"python -m venv .venv; .\\.venv\\Scripts\\Activate.ps1; python -m pip install --upgrade pip; pip install -r servers/requirements.txt\"",
    "test": "jest",
    "test:unit": "jest --runInBand",
    "test:api": "ts-node tests/test-real-api.ts",
    "test:key": "ts-node tests/test-api-key.ts",
    "test:imports": "ts-node tests/test-import.ts",
    "test:embeddings": "ts-node tests/test-real-api.ts",
    "test:mcp": "ts-node tests/test-mcp-http.ts",
    "clean": "node scripts/dev-utils.cjs clean",
    "setup": "node scripts/dev-utils.cjs setup",
    "docs": "node scripts/dev-utils.cjs docs",
    "mcp:config": "ts-node scripts/generate-mcp-config.ts",
    "dev-mode": "node scripts/dev-utils.cjs dev",
    "create": "node scripts/dev-utils.cjs create",
    "new:test": "node scripts/dev-utils.cjs create test",
    "new:doc": "node scripts/dev-utils.cjs create doc",
    "new:script": "node scripts/dev-utils.cjs create script",
    "check-structure": "node scripts/ensure-structure.js scan",
    "structure-guide": "node scripts/ensure-structure.js guide",
    "create-file": "node scripts/ensure-structure.js create",
    "validate-structure": "node scripts/validate-project-structure.js",
    "fix-structure": "node scripts/fix-project-structure.js",
    "fix-structure:dry": "node scripts/fix-project-structure.js --dry-run",
    "fix-structure:interactive": "node scripts/fix-project-structure.js --interactive",
    "precommit": "npm run check:canonical-files && npm run validate-structure",
    "lint": "eslint . --ext .ts --ignore-pattern coreagent/vscode-extension/** --fix",
    "lint:check": "eslint . --ext .ts --ignore-pattern coreagent/vscode-extension/**",
    "type-check": "tsc --noEmit",
    "ui:type-check": "tsc -p ui/tsconfig.app.json --noEmit",
    "check:banned-metrics": "node scripts/guard-metrics.cjs",
    "check:canonical-files": "node scripts/guard-canonical-files.cjs",
    "verify": "npm run check:canonical-files && npm run check:banned-metrics && npm run type-check && npm run ui:type-check && npm run lint:check",
    "verify:runtime": "npm run verify && npm run smoke:runtime && npm run test:toolsets && npm run test:stdio",
    "verify:final": "ts-node scripts/final-verification.ts",
    "lint:extension": "npm --prefix coreagent/vscode-extension run lint",
    "build:extension": "npm --prefix coreagent/vscode-extension run compile",
    "verify:extension": "npm --prefix coreagent/vscode-extension run lint && npm --prefix coreagent/vscode-extension run compile",
    "guardrails": "node scripts/guardrails-scan.cjs",
    "smoke": "ts-node tests/canonical/unified-systems.smoke.test.ts",
    "story:new": "ts-node scripts/new-story.ts",
    "smoke:mcp": "ts-node tests/smoke/mcp-startup.smoke.ts",
    "smoke:runtime": "ts-node scripts/runtime-smoke.ts",
    "test:a2a-nlacs": "node -r ts-node/register tests/canonical/a2a-nlacs-canonical.test.ts",
    "demo:hello": "ts-node scripts/hello-a2a.ts",
    "test:toolsets": "ts-node tests/canonical/toolsets.e2e.ts",
    "test:stdio": "ts-node tests/canonical/stdio-framing.e2e.ts",
    "test:a2a-conformance": "ts-node tests/canonical/communication-conformance.test.ts",
    "test:a2a-invariants": "ts-node tests/canonical/communication-invariants.test.ts",
    "test:a2a": "ts-node scripts/run-a2a-tests.ts",
    "sync:readme": "node scripts/sync-readme-version.js",
    "verify:readme": "node scripts/verify-readme-version.js",
    "format": "prettier --write .",
    "ui:dev": "vite",
    "ui:build": "vite build",
    "ui:preview": "vite preview",
    "test:ops": "node ./tests/monitoring/operation-metrics-summary.test.ts"
  },
  "keywords": [
    "ai",
    "agent",
    "automation",
    "workflow",
    "mcp",
    "typescript",
    "react",
    "ui"
  ],
  "author": "Arne",
  "license": "MIT",
  "type": "commonjs",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=11.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArnBdev/OneAgent.git"
  },
  "bugs": {
    "url": "https://github.com/ArnBdev/OneAgent/issues"
  },
  "homepage": "https://github.com/ArnBdev/OneAgent#readme",
  "overrides": {
    "axios": "^1.9.0",
    "undici": "^5.29.0"
  },
  "dependencies": {
    "@chroma-core/default-embed": "^0.1.8",
    "@google/generative-ai": "^0.24.1",
    "@types/passport": "^1.0.17",
    "chalk": "^5.4.1",
    "chromadb": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "jsdom": "^26.1.0",
    "mem0ai": "^2.1.33",
    "openai": "^5.16.0",
    "passport": "^0.7.0",
    "simple-git": "^3.24.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
  "@tailwindcss/postcss": "^4.1.0",
    "@types/axios": "^0.14.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.9.0",
    "eslint": "^9.35.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
  "tailwindcss": "^4.1.12",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.5",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5"
  }
}
