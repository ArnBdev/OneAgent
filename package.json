{
  "name": "oneagent-core",
  "version": "0.1.0",
  "description": "OneAgent CoreAgent - ModulÃ¦r AI-agentplattform with UI",
  "type": "commonjs",
  "main": "dist/coreagent/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/coreagent/main.js",
    "server": "ts-node coreagent/server/index-simple.ts",
    "server:dev": "npx nodemon --exec ts-node coreagent/server/index-simple.ts",
    "server:build": "tsc && node dist/coreagent/server/index-simple.js",
    "server:mcp": "ts-node coreagent/server/index-simple-mcp.ts",
    "server:mcp:dev": "npx nodemon --exec ts-node coreagent/server/index-simple-mcp.ts",
    "server:bg": "node scripts/start-background-server.js start",
    "server:stop": "node scripts/start-background-server.js stop",
    "server:status": "node scripts/start-background-server.js status",
    "server:restart": "node scripts/start-background-server.js restart",
    "test": "node scripts/dev-utils.js test",
    "test:api": "ts-node tests/test-real-api.ts",
    "test:key": "ts-node tests/test-api-key.ts",
    "test:imports": "ts-node tests/test-import.ts",
    "test:embeddings": "ts-node tests/test-real-api.ts",
    "test:mcp": "ts-node tests/test-mcp-http.ts",
    "clean": "node scripts/dev-utils.js clean",
    "setup": "node scripts/dev-utils.js setup",
    "docs": "node scripts/dev-utils.js docs",
    "dev-mode": "node scripts/dev-utils.js dev",
    "create": "node scripts/dev-utils.js create",
    "new:test": "node scripts/dev-utils.js create test",
    "new:doc": "node scripts/dev-utils.js create doc",
    "new:script": "node scripts/dev-utils.js create script",
    "check-structure": "node scripts/ensure-structure.js scan",
    "structure-guide": "node scripts/ensure-structure.js guide",
    "create-file": "node scripts/ensure-structure.js create",
    "lint": "echo \"Linting setup needed - add ESLint configuration\"",
    "format": "echo \"Formatting setup needed - add Prettier configuration\"",
    "ui:dev": "vite",
    "ui:build": "vite build",
    "ui:preview": "vite preview"
  },
  "keywords": [
    "ai",
    "agent",
    "automation",
    "workflow",
    "mcp",
    "typescript",
    "react",
    "ui"
  ],
  "author": "Arne",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "lucide-react": "^0.263.1",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/ws": "^8.5.8",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "axios": "^1.9.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
    "vite": "^4.4.0"
  }
}
